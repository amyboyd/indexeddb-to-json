{
  "name": "indexeddb-to-json",
  "description": "Extract IndexedDB to JSON, from any Chrome or Electron database directory",
  "version": "0.0.5",
  "main": "src/cli/index.ts",
  "scripts": {
    "lint-fix": "tsc && prettier --write 'src/**/*.js' 'src/**/*.ts' 'src/**/*.html'; eslint src/**/*.js src/**/*.ts .eslintrc.js --fix",
    "lint-check": "tsc && prettier --check 'src/**/*.js' 'src/**/*.ts' 'src/**/*.html'; eslint src/**/*.js src/**/*.ts .eslintrc.js",
    "lf": "yarn lint-fix",
    "lc": "yarn lint-check"
  },
  "dependencies": {
    "commander": "^6.0.0",
    "csv-writer": "^1.6.0",
    "fastify": "^3.2.1",
    "get-folder-size": "^2.0.1",
    "glob": "^7.1.6",
    "puppeteer": "^2.1.1",
    "recursive-copy": "https://github.com/amyboyd/recursive-copy#master",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/get-folder-size": "^2.0.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.6.0",
    "@types/puppeteer": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=13.3.0"
  },
  "files": [
    "src/",
    "yarn.lock",
    "README.md"
  ],
  "bin": "src/cli/index.ts"
}
